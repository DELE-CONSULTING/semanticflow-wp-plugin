{
  "name": "DELE-CONSULTING/semanticflow-wp-plugin",
  "type": "wordpress-plugin",
  "require": {
      "afragen/git-updater": "^12",
      "composer/installers": "^1"
  },
  "repositories": [
      {
          "type": "vcs",
          "url": "https://github.com/afragen/git-updater"
      }
  ],
  "extra": {
      "installer-paths": {
          "wp-content/plugins/{$name}/": [
              "type:wordpress-plugin"
          ]
      }
  }
}